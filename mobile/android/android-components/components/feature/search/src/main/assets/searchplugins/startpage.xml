<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
    <ShortName>Startpage</ShortName>
    <Image width="96" height="96">data:image/png;base64,</Image>
    <Url type="application/x-suggestions+json" rel="suggestions" template="https://www.startpage.com/osuggestions?q={searchTerms}"/>
    <!-- this is effectively x-moz-phonesearch, but search service expects a text/html entry -->
    <Url type="text/html" method="POST" template="https://www.startpage.com/sp/search">
        <Param name="query" value="{searchTerms}" />
        <Param name="segment" value="startpage.waterfox.1" />
        <Param name="cat" value="web" />
        <Param name="pl" value="opensearch" />
        <Param name="language" value="english" />
    </Url>
    <Url type="application/x-moz-tabletsearch" method="POST" template="https://www.startpage.com/sp/search">
        <Param name="query" value="{searchTerms}" />
        <Param name="segment" value="startpage.waterfox.1" />
        <Param name="cat" value="web" />
        <Param name="pl" value="opensearch" />
        <Param name="language" value="english" />
    </Url>
    <SearchForm>https://www.startpage.com/sp/search</SearchForm>
</SearchPlugin>
